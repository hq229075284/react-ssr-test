    "build": "concurrently \"npm run client:build\" \"npm run server:build\"",
    "server:watch": "nodemon -w ./dist_server ./dist_server/index.js",
    "server:compile": "babel -w --config-file ./babel.config.js ./server -d ./dist_server",
    "server": "concurrently \"npm run server:watch\" \"npm run server:webpack\""
